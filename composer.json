{
  "name": "darkweak/souin-api-platform-bundle",
  "description": "Souin HTTP cache bridge Api platform",
  "license": "MIT",
  "type": "symfony-bundle",
  "authors": [
    {
      "name": "darkweak",
      "email": "darkweak@protonmail.com"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "api-platform/core": "^2.1",
    "symfony/dependency-injection": "*",
    "guzzlehttp/guzzle": "^7",
    "symfony/config": "^5.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.2"
  },
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Darkweak\\SouinApiPlatformBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Darkweak\\SouinApiPlatformBundle\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "dev"
    }
  },
  "config": {
    "bin-dir": "bin"
  }
}
